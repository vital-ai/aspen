<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>vital-ai</groupId>
  <artifactId>install-jars</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>Install Vital AI Jars into Local Repo</name>
  
  
  <repositories>
  
    <repository>
      <id>vital-public-mvn-repo-releases</id>
      <name>Vital AI Public Maven Repo Releases</name>
      <url>https://github.com/vital-ai/vital-public-mvn-repo/raw/releases/</url>
    </repository>

    <repository>
      <id>vital-public-mvn-repo-snapshots</id>
      <name>Vital AI Public Maven Repo Snapshots</name>
      <url>https://github.com/vital-ai/vital-public-mvn-repo/raw/snapshots/</url>
    </repository>

  </repositories>
  


  <build>
    <plugins>

        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-install-plugin</artifactId>
            <executions>
                
                <execution>
                    <id>twentynews-groovy-1.0.0.jar</id>
                    <phase>validate</phase>
                    <goals>
                        <goal>install-file</goal>
                    </goals>
                    <configuration>
				      <groupId>vital-ai</groupId>
				      <artifactId>twentynews</artifactId>
				      <version>1.0.0</version>
				      <packaging>jar</packaging>
					  <file>${VITAL_HOME}/domain-groovy-jar/twentynews-groovy-1.0.0.jar</file>
                    </configuration>
                    
                </execution>
                
                
				<execution>
                    <id>movielens-groovy-1.0.0.jar</id>
                    <phase>validate</phase>
                    <goals>
                        <goal>install-file</goal>
                    </goals>
                    <configuration>
				      <groupId>vital-ai</groupId>
				      <artifactId>movielens</artifactId>
				      <version>1.0.0</version>
				      <packaging>jar</packaging>
					  <file>${VITAL_HOME}/domain-groovy-jar/movielens-groovy-1.0.0.jar</file>
                    </configuration>
                    
                </execution>
                
                
				<execution>
                    <id>autompg-groovy-1.0.0.jar</id>
                    <phase>validate</phase>
                    <goals>
                        <goal>install-file</goal>
                    </goals>
                    <configuration>
				      <groupId>vital-ai</groupId>
				      <artifactId>autompg</artifactId>
				      <version>1.0.0</version>
				      <packaging>jar</packaging>
					  <file>${VITAL_HOME}/domain-groovy-jar/autompg-groovy-1.0.0.jar</file>
                    </configuration>
                    
                </execution>

            </executions>
        </plugin>            
      
    </plugins>
  </build>
  

</project>