        <rdfs:domain rdf:resource="http://vital.ai/ontology/movielens#Movie"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://vital.ai/ontology/movielens#hasRating -->

    <owl:DatatypeProperty rdf:about="http://vital.ai/ontology/movielens#hasRating">
