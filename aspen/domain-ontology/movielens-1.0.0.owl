    <!-- http://vital.ai/ontology/movielens#Edge_hasMovieRating -->

    <owl:Class rdf:about="http://vital.ai/ontology/movielens#Edge_hasMovieRating">
        <rdfs:subClassOf rdf:resource="&vital-core;VITAL_PeerEdge"/>
